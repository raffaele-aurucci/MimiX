    "lite-server": "^2.6.1",
    "mime-types": "^2.1.27",
    "mini-css-extract-plugin": "^2.7.6",
    "path": "^0.12.7",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.7.3",
    "rimraf": "^5.0.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.3",
    "serve": "^14.2.0",
    "style-loader": "^1.2.1",
